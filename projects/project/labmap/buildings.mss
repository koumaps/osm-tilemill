#buildings, #historic{

    
  
   [zoom=15]{line-width: @buildings-width;
  line-color: @buildings-line;
  line-width: 0.2;}
    [zoom=14]{line-width: @buildings-width / (@road-zoom-reduce * 1);
  line-color: @buildings-line;
  line-width: 0.2;}
    [zoom=12]{line-width: @buildings-width / (@road-zoom-reduce * 2);
  line-color: @buildings-line;
  line-width: 0.2;}


 
 
  }

#historic{
  polygon-fill: @historic-color
    }

    


